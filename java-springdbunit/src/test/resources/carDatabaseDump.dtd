<!ELEMENT dataset (
    CAR*,
    CUSTOMER*)>

<!ELEMENT CAR EMPTY>
<!ATTLIST CAR
    ID CDATA #REQUIRED
    AVAILABILITY CDATA #REQUIRED
    CARBRAND CDATA #IMPLIED
    CREATETIME CDATA #IMPLIED
    UPDATETIME CDATA #IMPLIED
    YEAROFRENT CDATA #REQUIRED
    CUSTOMERID CDATA #IMPLIED
    CUSTOMERINDEX CDATA #IMPLIED
>

<!ELEMENT CUSTOMER EMPTY>
<!ATTLIST CUSTOMER
    ID CDATA #REQUIRED
    NAME CDATA #IMPLIED
    SURNAME CDATA #IMPLIED
>

